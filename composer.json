{
    "name": "goalgorilla/graphql-php-ws",
    "description": "GraphQL WebSocket middleware for Ratchet.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "bunny/bunny": "^0.4.3",
        "cboden/ratchet": "^0.4.3",
        "psr/log": "^1",
        "webonyx/graphql-php": "^14.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "GraphQLWs\\": "src" }
    },
    "extra": {
      "patches": {
        "webonyx/graphql-php": {
          "PR #755 Implement Utils::getOperationAST": "patches/graphql-php-add-getoperationast-pr-755.diff"
        }
      }
    }
}
